import requests

def send_task():
    payload = {
  "email": "student@example.com",
  "secret": "a46mnl09p7n9",
  "round": 1,
    }
    
    response = requests.post("https://laughing-broccoli-x5x9r775pwrwc6j7g-8000.app.github.dev/handle_task", json=payload)
    print(response.json())

if __name__ == "__main__":
    send_task()